{
	"version": 3,
		"functions": {
		"_render.func": {
		"runtime": "nodejs18.x",
		"handler": "_render.func/index.mjs",
		"maxDuration": 10
		}
	},
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/endpointEncuesta/?$",
			"dest": "_render"
		},
		{
			"src": "^/InicioSesion/?$",
			"dest": "_render"
		},
		{
			"src": "^/nosotros/?$",
			"dest": "_render"
		},
		{
			"src": "^/registrate/?$",
			"dest": "_render"
		},
		{
			"src": "^/tusCursos/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{ "src": "/(.*)", "dest": "_render" }
	]
}